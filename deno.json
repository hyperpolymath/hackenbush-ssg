{
  "name": "hackenbush-ssg",
  "version": "0.1.0",
  "exports": "./src/main.js",
  "tasks": {
    "build": "deno run --allow-read --allow-write src/main.js build",
    "serve": "deno run --allow-read --allow-net src/main.js serve",
    "dev": "deno run --watch --allow-read --allow-write --allow-net src/main.js dev",
    "evolve": "deno run --allow-read --allow-write src/main.js evolve",
    "test": "deno test --allow-read --allow-write",
    "lint": "deno lint",
    "fmt": "deno fmt"
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true
  }
}
